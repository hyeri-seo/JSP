package dto;

public class Account {
	private String id;
	private String name;
	private Integer balance;
	private String type;
	private String grade;

	public Account() {}
	public Account(String id, String name, Integer balance, String type, String grade) {
		this.id = id;
		this.name = name;
		this.balance = balance;
		this.type = type;
		this.grade = grade;
	}
	public void deposit(Integer money) {
		if(money>0) {
			balance += money;
		}
	}
}
